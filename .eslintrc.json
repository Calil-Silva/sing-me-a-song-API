{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "import/prefer-default-export": 0,
        "operator-linebreak":"off",
        "implicit-arrow-linebreak":"off",
        "camelcase":"off"
    }
}
